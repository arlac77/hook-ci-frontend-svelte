{
  "name": "svelte-playground",
  "version": "0.0.0-semantic-release",
  "description": "some description",
  "scripts": {
    "deploy": "scp public/* root@odroid1:/services/hook-ci/frontend-svelte/",
    "prepare": "rollup -c",
    "start": "rollup -c -w"
  },
  "dependencies": {
    "mf-styling": "git+https://github.com/arlac77/mf-styling.git",
    "svelte-guard-history-router": "git+https://github.com/arlac77/svelte-guard-history-router.git",
    "svelte-session-manager": "git+https://github.com/arlac77/svelte-session-manager.git"
  },
  "devDependencies": {
    "@semantic-release/exec": "^3.3.6",
    "apollo-boost": "^0.4.3",
    "apollo-client": "^2.6.3",
    "graphql": "^14.5.3",
    "light-server": "git+https://github.com/arlac77/light-server.git",
    "npm-pkgbuild": "^6.9.1",
    "rollup": "^1.20.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.1.0",
    "rollup-plugin-terser": "^5.1.1",
    "semantic-release": "^15.13.24",
    "svelte": "^3.9.2",
    "svelte-apollo": "^0.3.0"
  },
  "engines": {
    "node": ">=12.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/svelte-playground.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/svelte-playground/issues"
  },
  "homepage": "https://github.com/arlac77/svelte-playground#readme",
  "config": {
    "urlPrefix": "/services/ci/svelte",
    "api": "/services/ci/api",
    "proxyTarget": "https://mfelten.dynv6.net/",
    "graphQl": "/services/ci/api/graphql"
  },
  "pacman": {
    "groups": "webapp",
    "hooks": "pacman/install.sh",
    "installdir": "/services/hook-ci/frontend-svelte",
    "content": {
      "${installdir}/": {
        "base": "public"
      },
      "/etc/nginx/config.d/${name}.conf": "pacman/nginx.conf"
    },
    "depends": {
      "nginx-mainline": ">=1.17.3",
      "hook-ci": ">=1.41.8"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/exec",
        {
          "publishCmd": "npx npm-pkgbuild --pkgver ${nextRelease.version} pkgbuild pacman makepkg"
        }
      ]
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-svelte-app.git"
    }
  }
}
