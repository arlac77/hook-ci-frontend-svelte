{
  "name": "hook-ci-frontend-svelte",
  "version": "0.0.0-semantic-release",
  "description": "hook-ci web fronend",
  "keywords": [
    "svelte"
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "deploy": "scp public/* root@odroid1:/services/hook-ci/frontend/",
    "prepare": "rollup -c",
    "start": "rollup -c -w"
  },
  "dependencies": {
    "mf-styling": "arlac77/mf-styling",
    "repository-provider": "^16.8.6",
    "svelte-common": "^1.1.0",
    "svelte-guard-history-router": "^1.1.7",
    "svelte-session-manager": "^1.0.2"
  },
  "devDependencies": {
    "@semantic-release/exec": "^3.3.8",
    "apollo-boost": "^0.4.3",
    "apollo-client": "^2.6.3",
    "graphql": "^14.5.3",
    "npm-pkgbuild": "^6.10.4",
    "rollup": "^1.26.4",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-dev": "^1.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.1.0",
    "rollup-plugin-terser": "^5.1.2",
    "semantic-release": "^15.13.30",
    "svelte": "^3.12.1",
    "svelte-apollo": "^0.3.0"
  },
  "engines": {
    "node": ">=12.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/hook-ci-frontend-svelte.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/hook-ci-frontend-svelte/issues"
  },
  "homepage": "https://github.com/arlac77/hook-ci-frontend-svelte#readme",
  "config": {
    "base": "/services/ci",
    "api": "/services/ci/api",
    "proxyTarget": "https://mfelten.dynv6.net/",
    "graphQl": "/services/ci/api/graphql"
  },
  "pacman": {
    "groups": "webapp",
    "hooks": "pacman/hooks.sh",
    "installdir": "/services/hook-ci/frontend",
    "content": {
      "${installdir}/": {
        "base": "public"
      },
      "/etc/nginx/config.d/${name}.conf": "pacman/nginx.conf"
    },
    "depends": {
      "nginx-mainline": ">=1.17.4",
      "hook-ci": ">=1.51.10"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/exec",
        {
          "publishCmd": "npx npm-pkgbuild --pkgver ${nextRelease.version} pkgbuild pacman makepkg"
        }
      ]
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-svelte-app.git"
    }
  }
}
