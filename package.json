{
  "name": "hook-ci-frontend-svelte",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "description": "hook-ci web fronend",
  "keywords": [
    "hook-ci",
    "svelte",
    "web"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "prepare": "rollup -c rollup.config.mjs",
    "start": "rollup -c -w rollup.config.mjs"
  },
  "dependencies": {
    "mf-styling": "arlac77/mf-styling",
    "microtip": "^0.2.2",
    "repository-provider": "^27.0.0",
    "svelte-common": "^4.4.21",
    "svelte-guard-history-router": "^4.5.2",
    "svelte-session-manager": "^2.0.10"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "npm-pkgbuild": "^7.25.2",
    "semantic-release": "^19.0.2",
    "svelte": "^3.46.3",
    "svql": "0.0.35"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/hook-ci-frontend-svelte.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/hook-ci-frontend-svelte/issues"
  },
  "homepage": "https://github.com/arlac77/hook-ci-frontend-svelte#readme",
  "config": {
    "api": "/services/ci/api",
    "base": "/services/ci",
    "graphQl": "/services/ci/api/graphql",
    "proxyTarget": "https://mfelten.dynv6.net/",
    "title": "Appliction Name"
  },
  "pkg": {
    "groups": "webapp",
    "hooks": "pkg/hooks.sh",
    "installdir": "/services/hook-ci/frontend",
    "content": {
      "${installdir}/": {
        "base": "public"
      },
      "/etc/nginx/sites/common/${name}.conf": "pkg/nginx.conf"
    },
    "depends": {
      "nginx-mainline": ">=1.21.4",
      "hook-ci": ">=1.56.0"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/exec",
        {
          "publishCmd": "npx npm-pkgbuild"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-svelte-app#next"
    ]
  }
}
