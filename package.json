{
  "name": "hook-ci-frontend-svelte",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "description": "hook-ci web fronend",
  "keywords": [
    "hook-ci",
    "svelte",
    "vite",
    "web"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "prepare": "vite build",
    "start": "rollup -c -w rollup.config.mjs && vite",
    "dev": "vite",
    "serve": "vite preview"
  },
  "dependencies": {
    "mf-styling": "arlac77/mf-styling",
    "microtip": "^0.2.2",
    "repository-provider": "^28.0.0",
    "svelte-common": "^4.4.32",
    "svelte-guard-history-router": "^4.6.4",
    "svelte-session-manager": "^2.0.10"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.36",
    "npm-pkgbuild": "^8.3.17",
    "semantic-release": "^19.0.2",
    "svelte": "^3.47.0",
    "svql": "0.0.35",
    "vite": "^2.8.2"
  },
  "optionalDependencies": {
    "mf-hosting": "github:arlac77/mf-hosting"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/hook-ci-frontend-svelte.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/hook-ci-frontend-svelte/issues"
  },
  "homepage": "https://github.com/arlac77/hook-ci-frontend-svelte#readme",
  "config": {
    "api": "/services/ci/api",
    "base": "/services/ci",
    "graphQl": "/services/ci/api/graphql",
    "proxyTarget": "https://mfelten.dynv6.net/",
    "title": "Appliction Name"
  },
  "pkg": {
    "content": {
      "${install.dir}": {
        "base": "public"
      },
      "${nginx.sites.dir}${name}.conf": "pkg/nginx.conf"
    },
    "hooks": "pkg/hooks.sh",
    "groups": "webapp",
    "depends": {
      "nginx-mainline": ">=1.21.4",
      "hook-ci": ">=1.56.0"
    },
    "install.dir": "/services/hook-ci/frontend/"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/exec",
        {
          "publishCmd": "npx npm-pkgbuild"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-svelte-app#next"
    ]
  }
}
